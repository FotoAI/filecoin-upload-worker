# Filecoin Upload Worker Environment Variables
# Copy this file to .env for local development (optional)
# For production, use wrangler secrets instead

# Required: Private key for Filecoin wallet (use wrangler secret put FILECOIN_PRIVATE_KEY)
FILECOIN_PRIVATE_KEY=your_filecoin_private_key_here

# Required: Backend API URL for upload notifications (use wrangler secret put BACKEND_API_URL)
BACKEND_API_URL=https://your-backend-api.com/api/upload-notification

# Optional: Bearer token for backend API authentication (use wrangler secret put BACKEND_API_KEY)
BACKEND_API_KEY=your_backend_api_key_here

# Optional: Filecoin RPC URL (defaults to calibration network)
FILECOIN_RPC_URL=https://api.calibration.node.glif.io/rpc/v1

# Optional: Filecoin network (defaults to "calibration")
FILECOIN_NETWORK=calibration

# Optional: CDN base URL (defaults to "https://pdp-test.thcloud.dev")
THCLOUD_API_BASE=https://pdp-test.thcloud.dev